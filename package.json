{
  "name": "cra-template-mugilan",
  "version": "0.1.0",
  "description": "The base template for Mugilan's CRA",
  "private": true,
  "author": "Mugilan E.S. <mugilancodes@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "clean-files": "rm -rf template/public template/src && rm -rf template/jsconfig.json template/README.md",
    "copy-files": "cp -a src/ template/src && cp -a public/ template/public && cp jsconfig.json README.md template/",
    "prepublishOnly": "npm run clean-files && npm run copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mugilan-Codes/cra-template-mugilan.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/Mugilan-Codes/cra-template-mugilan/issues"
  },
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "cra-template",
    "react-router-dom"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  }
}
